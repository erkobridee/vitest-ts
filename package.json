  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.10.12",
    "esbuild": "0.27.3",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.1.0",
    "vitest": "4.0.18"
